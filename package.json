    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "webpack": "webpack",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "rx:build": "npm run webpack -- --env.mode production",
    "rx:bundle": "rm -rf /build && cross-env NODE_ENV=emotion babel apps/rx-xis-app/src/components -d build",
    "rx:publish": "yarn publish build",
    "rx:storybook": "cd ./apps/rx-xis-app && start-storybook -p 6006",
    "rx:build-storybook": "cd ./apps/rx-xis-app && build-storybook -o ../../dist/storybook/react",
    "ng:a11y": "a11y http://localhost:4200/",
    "angular:docs:json": "cd libs/angular/ && compodoc -p ./tsconfig.json -e json -d .",
    "remove-all": "rm -rf node_modules/ && rm -rf libs/static/node_modules/ &&  rm -rf apps/rx-xis-app/node_modules/",
    "install-all": "yarn install && cd libs/static/ && yarn install && cd ../../apps/rx-xis-app/ && yarn install",
    "build:libs": "rm -rf dist && ng-packagr -p libs/angular/src/ng-package.json && cp -r $(pwd)/libs/static/. $(pwd)/dist/@mobilelivenpm/fds-static-scss && cp -r $(pwd)/libs/icons/. $(pwd)/dist/@mobilelivenpm/fds-icons",
    "build:static-docs": "cd libs/static/ && npm run build && cp -r $(pwd)/dist/. $(pwd)/../../dist/static",
    "build:angular-examples-relative-path": "nx build --prod --base-href \"/apps/angular-demos/\" angular-demos && nx build --prod --base-href \"/apps/angular-sign-up/\" angular-sign-up",
    "build-all-docs": "rm -rf dist/ && npm run rx:build-storybook && nx run angular:build-storybook-with-docs && npm run build:static-docs && npm run build:angular-examples-relative-path"
  },
